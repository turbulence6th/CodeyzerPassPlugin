{
  "name": "Codeyzer Pass",
  "description": "__MSG_codeyzer_app_aciklama__",
  "version": "1.3.2",
  "default_locale": "en",
  "manifest_version": 3,
  "permissions": ["declarativeContent", "activeTab", "storage"],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["lib/jquery-ui.css", "contentScript/contentScript.css"],
      "js": [
        "node_modules/jquery/dist/jquery.js", 
        "lib/jquery-ui.js", 
        "/node_modules/@webcomponents/custom-elements/custom-elements.min.js", 
        "contentScript/contentScriptLoader.js"
      ],
      "all_frames": false
    }
  ],
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/contentScript/contentScript.js",
        "/contentScript/SifreEklePanel.js",
        "/node_modules/bootstrap/dist/css/bootstrap.css",
        "/core/*",
        "/popup/*",
        "/images/*",
        "/i18n/*",
        "/iframe/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}