{
  "name": "Codeyzer Pass",
  "version": "1.2.0",
  "description": "Kripto ÅŸifre saklama eklentisidir",
  "manifest_version": 3,
  "permissions": ["declarativeContent", "activeTab", "clipboardWrite", "storage"],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["lib/jquery-ui.css", "contentScript/contentScript.css"],
      "js": ["node_modules/jquery/dist/jquery.js", "lib/jquery-ui.js", "contentScript/contentScriptLoader.js"],
      "all_frames": false
    }
  ],
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "core/util.js",
        "core/bilesenler/CodeyzerBilesen.js",
        "core/AygitYonetici.js",
        "contentScript/contentScript.js",
        "kaynak/i18n/tr.js",
        "kaynak/i18n/en.js",
        "iframe/autocomplete/autocomplete.html",
        "iframe/beniAc/beniAc.html"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}