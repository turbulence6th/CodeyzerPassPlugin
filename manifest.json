{
  "name": "Codeyzer Pass",
  "version": "1.0.1",
  "description": "Kripto ÅŸifre saklama eklentisidir",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "activeTab"],
  "background": {
    "scripts": ["lib/jquery.js", "background.js"],
    "persistent": true
  },
  "page_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["blank.css"],
      "js": ["lib/jquery.js", "contentScript.js"],
      "all_frames": false
    }
  ],
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  }
}